  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Geoweaver Command Line",
      "request": "launch",
      "mainClass": "com.gw.GeoweaverApplication",
      "args": "${command:SpecifyProgramArgs}"
    },
    {
      "type": "java",
      "name": "Launch BaseTool",
      "request": "launch",
      "mainClass": "com.gw.utils.BaseTool",
      "projectName": "geoweaver"
    },
    {
      "type": "java",
      "name": "Launch ResetPassword",
      "request": "launch",
      "mainClass": "com.gw.tools.ResetPassword",
      "projectName": "geoweaver"
    },
    {
      "type": "java",
      "name": "Launch TempTest",
      "request": "launch",
      "mainClass": "com.TempTest",
      "projectName": "geoweaver"
    },
    {
      "type": "java",
      "name": "Launch GmailAPI",
      "request": "launch",
      "mainClass": "com.gw.utils.GmailAPI",
      "projectName": "geoweaver"
    },
    {
      "type": "java",
      "name": "Launch GeoweaverApplication",
      "request": "launch",
      "mainClass": "com.gw.GeoweaverApplication",
      "projectName": "geoweaver",
      "args": "resetpassword"
    }
  ]
}
